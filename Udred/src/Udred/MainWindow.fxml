<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<BorderPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Udred.MainController">
   <center>
      <GridPane maxWidth="1.7976931348623157E308">
         <columnConstraints>
            <ColumnConstraints hgrow="ALWAYS" maxWidth="800.0" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints fillHeight="false" vgrow="NEVER" />
         </rowConstraints>
         <children>
             <fx:include fx:id="embeddedCaseView" maxWidth="1.7976931348623157E308" source="CaseEditor.fxml" />
         </children>
      </GridPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="Fil">
            <items>
                  <MenuItem mnemonicParsing="false" text="Opret Sag" />
              <MenuItem mnemonicParsing="false" text="Åben Sag" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <MenuItem mnemonicParsing="false" text="Luk" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Rediger">
            <items>
              <MenuItem mnemonicParsing="false" text="Klip" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Hjælp">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
